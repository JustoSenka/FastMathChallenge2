<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.JustInGApps.fastmathchallenge2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        <activity
            android:name="com.JustInGApps.fastmathchallenge2.main.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="sensorLandscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.JustInGApps.fastmathchallenge2.highscore.HighscoreActivity"
            android:screenOrientation="sensorLandscape" >
        </activity>
        <activity
            android:name="com.JustInGApps.fastmathchallenge2.highscore.AddHighscore"
            android:screenOrientation="sensorLandscape" >
        </activity>
    </application>

</manifest>